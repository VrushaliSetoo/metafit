{
  "collectionName": "components_slices_testimonials",
  "info": {
    "name": "Testimonial",
    "displayName": "Testimonial",
    "icon": "user-check",
    "description": ""
  },
  "options": {},
  "attributes": {
    "picture": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "text": {
      "type": "text",
      "required": true
    },
    "authorName": {
      "type": "string",
      "required": true
    },
    "designation": {
      "type": "string"
    },
    "fill": {
      "type": "customField",
      "regex": "^#([A-Fa-f0-9]{6}|[A-Fa-f0-9]{3})$",
      "customField": "plugin::color-picker.color"
    }
  }
}
