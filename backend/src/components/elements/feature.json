{
  "collectionName": "components_elements_features",
  "info": {
    "displayName": "Feature",
    "description": ""
  },
  "options": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "description": {
      "type": "text"
    },
    "media": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "showLink": {
      "type": "boolean",
      "default": false
    },
    "newTab": {
      "type": "boolean",
      "default": false
    },
    "url": {
      "type": "string"
    },
    "text": {
      "type": "string"
    },
    "colour": {
      "type": "customField",
      "regex": "^#([A-Fa-f0-9]{6}|[A-Fa-f0-9]{3})$",
      "customField": "plugin::color-picker.color"
    }
  }
}
